<?xml version="1.0"?>
<launch>
  <!-- 
    Mission Planner Launch File
    This launch file starts the mission_manager node for autonomous operations
  -->

  <!-- Launch the mission_manager node -->
  <node pkg="autonomy" type="mission_manager.py" name="mission_manager" output="screen">
    <remap from="/mission_status" to="/mission_status" />
  </node>
</launch>